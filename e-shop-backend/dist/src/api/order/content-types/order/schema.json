{
    "kind": "collectionType",
    "collectionName": "orders",
    "info": {
        "singularName": "order",
        "pluralName": "orders",
        "displayName": "order"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "status": {
            "type": "enumeration",
            "enum": [
                "paid",
                "unpaid"
            ]
        },
        "total": {
            "type": "decimal"
        },
        "checkout_session": {
            "type": "string"
        },
        "product": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::product.product",
            "inversedBy": "orders"
        },
        "user": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "plugin::users-permissions.user",
            "inversedBy": "orders"
        }
    }
}
